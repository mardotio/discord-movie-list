{
  "compilerOptions": {
    "module": "CommonJS",
    "lib": [
      "es6"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [],
    "noEmit": false,
    "forceConsistentCasingInFileNames": true,
    "outDir": "lib",
    "declaration": true
  },
  "include": ["src", "index.ts"]
}
