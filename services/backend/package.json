{
  "name": "discord-movie-list-backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.ts",
    "run": "wait-on build/server.js && nodemon build/server.js",
    "start": "concurrently \"npm run build\" \"npm run run\"",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.ts",
    "start:prod": "node build/server.js",
    "lint": "eslint --ext .js --ext .ts src",
    "format": "./node_modules/prettier/bin-prettier.js --loglevel silent --write ."
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.21.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.19.7",
    "@types/node-fetch": "^2.5.8",
    "concurrently": "^5.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.0",
    "typescript": "^4.1.2",
    "wait-on": "^5.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
